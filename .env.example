# Discord bot token
DISCORD_TOKEN=YOUR_TOKEN_HERE

# Command prefix (default: !)
COMMAND_PREFIX=!

# Minecraft server for !stats minecraft (host or host:port)
MINECRAFT_SERVER=adress:25565

# ARK server (Steam QueryPort / A2S)
ARK_ETST1_SERVER=adress:27015

# Satisfactory server (QueryPort / A2S)
# Used for: !stats satisfactory
# Default expected port: 8888
SATISFACTORY_SERVER=adress:8888

# Tracker Network API (https://tracker.gg/developers)
# Used for: !stats smite1 <pseudo>, !stats smite2 <pseudo>
TRN_API_KEY=YOUR_TRN_APP_ID_HERE

# Default platforms (optional). You can also do: steam:PlayerName
TRN_SMITE1_PLATFORM=steam
TRN_SMITE2_PLATFORM=steam

# Rocket League via RapidAPI
RAPIDAPI_KEY=YOUR_RAPIDAPI_KEY_HERE
RL_RAPIDAPI_HOST=rocket-league1.p.rapidapi.com
# Can be a full URL or a path (starting with '/') with placeholders {platform} and {identifier}
RL_RAPIDAPI_URL_TEMPLATE=/ranks/{identifier}
RL_PLATFORM=epic

# (optionnel) Les pseudos Steam/Epic peuvent être enregistrés en Discord avec la commande `!id`.

# Supercell official APIs (require IP whitelisting on the developer portal)
# Used for: !stats coc #TAG, !stats brawl #TAG
COC_API_TOKEN=YOUR_COC_API_TOKEN_HERE
BRAWLSTARS_API_TOKEN=YOUR_BRAWLSTARS_API_TOKEN_HERE
